<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <RootNamespace>GLTech2</RootNamespace>
    <UseWindowsForms>true</UseWindowsForms>
    <StartupObject>Test.ProgramMain</StartupObject>
    <Nullable>annotations</Nullable>
    <AnalysisLevel>5.0</AnalysisLevel>
    <AssemblyVersion>2.1.0.0</AssemblyVersion>
    <FileVersion>2.1.0.0</FileVersion>
    <ApplicationIcon />
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <PlatformTarget>x64</PlatformTarget>
    <OutputPath>..\bin\Debug</OutputPath>
    <DocumentationFile>..\bin\Debug\GL Tech 2.1.xml</DocumentationFile>
    <NoWarn>1701;1702;CS1592</NoWarn>
    <WarningsAsErrors>;NU1605</WarningsAsErrors>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <PlatformTarget>x64</PlatformTarget>
    <OutputPath>..\bin\Release</OutputPath>
    <DocumentationFile>..\bin\Release\GL Tech 2.1.xml</DocumentationFile>
    <NoWarn>1701;1702;CS1592</NoWarn>
    <WarningsAsErrors>;NU1605</WarningsAsErrors>
  </PropertyGroup>

  <ItemGroup>
    <None Include="Elements\Gridmap.Bindings.cs" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Test\DemoTextures.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>DemoTextures.resx</DependentUpon>
    </Compile>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
    <Compile Update="VersionData.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>VersionData.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Test\DemoTextures.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>DemoTextures.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="VersionData.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>VersionData.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>